{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://schema.waveframe.org/awo/model_fingerprint.schema.json",
  "title": "AWO Model Fingerprint",
  "type": "object",
  "required": [
    "provider",
    "model_id",
    "endpoint",
    "engine_build",
    "created_at",
    "context_window",
    "temperature",
    "top_p",
    "max_output_tokens",
    "seed",
    "stop_tokens",
    "system_prompt_hash",
    "tool_schema_hash",
    "prompt_serialization_hash",
    "sdk_name",
    "sdk_version",
    "runtime_env",
    "privacy_mode"
  ],
  "properties": {
    "provider": { "type": "string", "examples": ["openai","anthropic","google","local"] },
    "model_id": { "type": "string", "examples": ["gpt-5.1","claude-3.7"] },
    "endpoint": { "type": "string", "description": "Resolved base URL or routing label" },
    "engine_build": { "type": "string", "description": "Vendor engine build/revision if exposed" },
    "created_at": { "type": "string", "format": "date-time" },
    "context_window": { "type": "integer", "minimum": 1 },
    "temperature": { "type": "number", "minimum": 0 },
    "top_p": { "type": "number", "minimum": 0, "maximum": 1 },
    "top_k": { "type": "integer", "minimum": 0 },
    "max_output_tokens": { "type": "integer", "minimum": 1 },
    "seed": { "type": ["integer","string"], "description": "Determinism hint; may be ignored by provider" },
    "stop_tokens": { "type": "array", "items": { "type": "string" } },
    "frequency_penalty": { "type": "number" },
    "presence_penalty": { "type": "number" },
    "system_prompt_hash": { "type": "string", "pattern": "^[a-f0-9]{64}$" },
    "tool_schema_hash": { "type": "string", "pattern": "^[a-f0-9]{64}$" },
    "prompt_serialization_hash": { "type": "string", "pattern": "^[a-f0-9]{64}$" },
    "tool_plugins": { "type": "array", "items": { "type": "string" } },
    "sdk_name": { "type": "string", "examples": ["openai-sdk","anthropic-sdk","http"] },
    "sdk_version": { "type": "string" },
    "runtime_env": {
      "type": "object",
      "required": ["language","version","os"],
      "properties": {
        "language": { "type": "string", "examples": ["python","node"] },
        "version": { "type": "string", "examples": ["3.11.9","22.11.0"] },
        "os": { "type": "string" }
      }
    },
    "hardware_hint": { "type": "string", "description": "e.g., A100, TPUv5e, CPU" },
    "latency_ms": { "type": "number" },
    "cost_estimate": { "type": "number", "description": "Optional per-call USD estimate" },
    "privacy_mode": { "type": "string", "enum": ["public","private","bring-your-own-endpoint"] }
  },
  "additionalProperties": false
}
